    "start": "node app.js",
    "create": "babel-node ./server/models/tables.js",
    "seed-admin": "babel-node ./server/models/user.js"
  },
  "engines": {
    "node": "12.4.0"
  },
  "author": "Gustave Harintwari",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "@hapi/joi": "^15.1.1",
    "bcrypt": "^3.0.8",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1",
    "pg": "^7.18.2",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
