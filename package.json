{
  "name": "server",
  "version": "1.0.0",
  "description": "Free Mentors",
  "main": "app.js",
  "directories": {
    "test": "tests"
  },
  "nyc": {
    "exclude": [
      "server/tests/*",
      "server.js"
    ]
  },
  "scripts": {
    "test": "nyc mocha --require @babel/register ./server/tests/* --timeout 10000 --exit || true",
